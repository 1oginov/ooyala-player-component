{
  "name": "ooyala-player-component",
  "version": "0.1.0",
  "description": "Ooyala Player Component",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1oginov/ooyala-player-component.git"
  },
  "keywords": [
    "ooyala",
    "player",
    "component"
  ],
  "author": "Danila Loginov <danila@loginov.rocks> (https://loginov.rocks)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1oginov/ooyala-player-component/issues"
  },
  "homepage": "https://github.com/1oginov/ooyala-player-component",
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "^0.12.2",
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
